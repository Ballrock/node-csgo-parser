{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["package.json", "./src/"],
        "excludePattern": "(node_modules/|docs|test/|out/)"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "./out/docs/",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "./node_modules/minami"
    }
}